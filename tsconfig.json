{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./src",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@api/*": [
        "./api/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@navigation/*": [
        "./navigation/*"
      ]
    }
  },
      
  "include": [
    "src/**/*",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
